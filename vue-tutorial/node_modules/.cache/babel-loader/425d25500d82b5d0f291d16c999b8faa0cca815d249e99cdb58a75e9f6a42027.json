{"ast":null,"code":"import Modal from \"./components/Modal.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    Modal\n  },\n  data() {\n    return {\n      title: \"First Vue App :P\",\n      header: \"Header Hi\",\n      content: \"This is the content\",\n      showModal: false,\n      showModal2: false\n    };\n  },\n  methods: {\n    handleClick() {\n      console.log(this.$refs.name);\n      this.$refs.name.classList.add(\"active\");\n      this.$refs.name.focus();\n    },\n    toggleModal() {\n      this.showModal = !this.showModal;\n    },\n    toggleModal2() {\n      this.showModal2 = !this.showModal2;\n    }\n  }\n};","map":{"version":3,"names":["Modal","name","components","data","title","header","content","showModal","showModal2","methods","handleClick","console","log","$refs","classList","add","focus","toggleModal","toggleModal2"],"sources":["/Users/kityuenchan/VueJs/vue-tutorial/src/App.vue"],"sourcesContent":["<template>\n  <h1>{{ title }}</h1>\n  <input type=\"text\" ref=\"name\" />\n  <button @click=\"handleClick\">Click me</button>\n  <teleport to=\".modal\" v-if=\"showModal\">\n    <Modal :header=\"header\" :content=\"content\" theme=\"\" @close=\"toggleModal\">\n      <template v-slot:links>\n        <a href=\"#\">Sign Up Now</a>\n        <a href=\"#\">More Info</a>\n      </template>\n    <p>Hi im slot, im being pass through from outside of this component!</p>\n    </Modal>\n  </teleport>\n  <button @click.alt=\"toggleModal\">Open Modal 1(alt + click)</button>\n\n  <teleport to=\".modal\" v-if=\"showModal2\">\n    <Modal :header=\"header\" :content=\"content\" theme=\"\" @close=\"toggleModal2\">\n      \n    <p>Hi im slot, and im exist in modal 2!</p>\n    </Modal>\n  </teleport>\n\n  <button @click=\"toggleModal2\">Open Modal 2</button>\n\n</template>\n\n<script>\nimport Modal from \"./components/Modal.vue\";\n\nexport default {\n  name: \"App\",\n  components: { Modal },\n  data() {\n    return {\n      title: \"First Vue App :P\",\n      header: \"Header Hi\",\n      content: \"This is the content\",\n      showModal: false,\n      showModal2: false\n    };\n  },\n  methods: {\n    handleClick() {\n      console.log(this.$refs.name);\n      this.$refs.name.classList.add(\"active\");\n      this.$refs.name.focus();\n    },\n    toggleModal(){\n      this.showModal = !this.showModal\n    },\n    toggleModal2(){\n      this.showModal2 = !this.showModal2\n    }\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"],"mappings":"AA2BA,OAAOA,KAAI,MAAO,wBAAwB;AAE1C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IAAEF;EAAM,CAAC;EACrBG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,kBAAkB;MACzBC,MAAM,EAAE,WAAW;MACnBC,OAAO,EAAE,qBAAqB;MAC9BC,SAAS,EAAE,KAAK;MAChBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,KAAK,CAACZ,IAAI,CAAC;MAC5B,IAAI,CAACY,KAAK,CAACZ,IAAI,CAACa,SAAS,CAACC,GAAG,CAAC,QAAQ,CAAC;MACvC,IAAI,CAACF,KAAK,CAACZ,IAAI,CAACe,KAAK,CAAC,CAAC;IACzB,CAAC;IACDC,WAAWA,CAAA,EAAE;MACX,IAAI,CAACV,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAQ;IACjC,CAAC;IACDW,YAAYA,CAAA,EAAE;MACZ,IAAI,CAACV,UAAS,GAAI,CAAC,IAAI,CAACA,UAAS;IACnC;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}